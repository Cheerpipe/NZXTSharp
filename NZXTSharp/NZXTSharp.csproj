<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1.2-beta</Version>
    <Description>The one-stop C# sdk that facilitates interaction with NZXT devices.</Description>
    <Authors>Ari Madian</Authors>
    <Company></Company>
    <PackageProjectUrl>https://github.com/akmadian/NZXTSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/akmadian/NZXTSharp</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>NZXT, Hue+, Hue_Plus, Hue, Plus, SDK, NZXT_SDK, RGB, RGB_SDK</PackageTags>
    <PackageLicenseUrl>https://github.com/akmadian/NZXTSharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>https://i.imgur.com/4GAobyk.jpg</PackageIconUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyVersion>1.0.1.2</AssemblyVersion>
    <FileVersion>1.0.1.2</FileVersion>
    <PackageReleaseNotes>Refactoring, various small fixes. No syntax changes.</PackageReleaseNotes>
    <Copyright>Copyright provided under the GNU GPLv3 open source license.</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Ports" Version="4.6.0-preview.18571.3" />
  </ItemGroup>

</Project>
